package Kakao;

import java.util.ArrayList;

class Solution3 {
    public int[] solution(int N, int[] stages) {
    	ArrayList<Integer> list = new ArrayList<>();
    	for(int i=1; i<=stages.length; i++) {
    		for(int j=1; j<=N; j++) {
        		int sum=0;
    			if(stages[i] == j) {
    				sum++;
    			}
				if(list.isEmpty()) {
					list.add(j);
				}else if(list.get(0)>sum/stages.length) {
					list.add(j);
				}else
	    			list.add(0, j);
    		}
    	}
    	
    	int[] result = new int[list.size()];
    	int idx = 0 ;
    	for(int n : list) {
    		result[idx] = n;
    		idx++;
    	}
    return result;
    }
}
