Student.java
public class Student {
	public int studentNumber;
	public String studentName;
	public int grade;
//default 생성자는 생성자가 하나도 없을 때만 만들어진다. 생성자가 팔요하다면 직접 넣으면 된다.
	public Student(){} //생성자 2개 오버로딩
	//생성을 할 때 이름 학번 학년을 매개변수로 받아서 초기화 해주고 싶을 때
	public Student(int studentNumber, String studentName, int grade){
this.studentNumber = studentNumber; //객체가 생성이 될 때 자기가 받은 파라미터 값을 멤버변수에 넣어주는 것.
	this.studentName = studentName;
this.grade =  grade;
}	
	public String showStudentInfo() {
		return studentName+ “학생의 학번은 “ + studentNumber + “이고” + grade +”입니다” + i;
}
}

클라이언트 코드/ 구동코드
public class StudentTest {
	public static void main(String[] args) {
	Student studentLee = new Student();
	System.out.println(studentLee.showStudentInfo());//멤버변수는 객체가 만들어질 때 자동으로 초기화된다. showStudentInfo에 지역변수를 넣고 돌리면 에러 (자동초기화 x)
	Student studentKim =new Student(1234, “Kim”, 3);
System.out.println(studentKim.showStudentInfo());
}
}
