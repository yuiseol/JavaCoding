package charIOstream;

import java.io.FileWriter;

public class FileWriterTest {

	public static void main(String[] args) {
		
		try(FileWriter fw = new FileWriter("writer.txt")) {
			
			fw.writer('A');
			char buf[] = {'B', 'C', 'D', 'E', 'F', 'G'};
			
			fw.write(buf);
			fw.write("안녕하세요 , 잘써지네요");
			fw.write(buf, 1, 2);
			fw.write("65");
		}
	
	}

}
