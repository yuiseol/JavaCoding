package dataStructure03;

import dataStructure01.MyLinkedList;
import dataStructure01.MyListNode;

interface Queue{
	public void enQueue(String data);
	public String deQueue();
	public void printQueue();
}

public class MyLinkedQueue extends MyLinkedList implements Queue {
	
	MyListNode front;
	MyListNode rear;

	@Override
	public void enQueue(String data) {
		
		MyListNode newNode;
		if(isEmpty()) {
			newNode = addElement(data);
			front = newNode;
			rear = newNode;
		}else {
			newNode = addElement(data);
			rear = newNode;
		}
		System.out.println(newNode.getData() + "added");
	}

	@Override
	public String deQueue() {
		
		
	}

	@Override
	public void printQueue() {
	
		
	}

}
